{
    "manifest_version": 2,
    "name": "MaskItSpidey",
    "description":"This extension chnages the background of your target website.",
    "version":"1.0",
    "icons": {
        "16":"/images/Logo16.png",
        "48":"/images/Logo.png",
        "128":"/images/Logo.png"
    },     
    "page_action": {
        "default_icon": {
            "16":"/images/Logo16.png",
            "48":"/images/Logo.png",
            "128":"/images/Logo.png"
        }

    },
    "content_scripts":[{
            "matches":["https://www.google.com/*"],
            "css":["main.css"]
        }]   

}

